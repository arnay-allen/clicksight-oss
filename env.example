# ClickHouse Connection
VITE_CLICKHOUSE_URL=https://your-clickhouse-host.com:8443
VITE_CLICKHOUSE_USER=default
VITE_CLICKHOUSE_PASSWORD=your_password
VITE_CLICKHOUSE_DATABASE=your_database

# Google OAuth (for authentication)
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Feature Flags
VITE_USE_LOWERCASE_COLUMNS=false

# OpenAI API (Optional - for Ask AI feature in SQL Playground)
# Get your API key from: https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=sk-your-openai-api-key-here
